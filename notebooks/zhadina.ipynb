    "        blockzone=[]\n",
    "        zone=pzone\n",
    "        prevmask=genmask()\n",
    "        times=[]\n",
    "        for i in range(16):\n",
    "            if zone[i] and not prevmask[i]:\n",
    "                times.append(random.randint(1,60))\n",
    "            else:\n",
    "                times.append(1)\n",
    "    else:\n",
    "        for i in range(16):\n",
    "            zone[i]+=pzone[i];\n",
    "    blockzone=[]\n",
    "    for i in range(4):\n",
    "        if prevmask[i+12]:\n",
    "            blockzone.append((random.randint(2,3)==2)*random.randint(0,6))\n",
