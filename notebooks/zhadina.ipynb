    "def noblock(mask,blockzone):\n",
    "    blockmask=[]\n",
    "    for i in range(12):\n",
    "        blockmask.append(0)\n",
    "    for j in range(0,4):\n",
    "        if(blockzone[j]):\n",
    "            for i in range(j*3,3+j*3):\n",
    "                blockmask[i]=1\n",
    "            for i in range(j*3+3,j*3+11,4):\n",
    "                blockmask[i%12]=1\n",
    "    for i in range(12):\n",
    "        if(mask[i] and blockmask[i]):\n",
    "            return 0\n",
    "    return 1"
   ]
