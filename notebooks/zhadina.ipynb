    "pt=time.time()\n",
    "\n",
    "def greedchoose(masksize, zone):\n",
    "    maxv=0\n",
    "    maxmask=[]\n",
    "    for i in range(1 << masksize):\n",
    "        mask = tomask(i)\n",
    "        a = greed(mask, zone) * possiblemask(mask)*noblock(mask,blockzone)\n",
    "        if(maxv <= a):\n",
    "            maxv = a\n",
    "            maxmask = mask\n",
    "    for i in range(16):\n",
    "        if (maxmask[i]):\n",
    "            times[i]=1\n",
    "            if(prevmask[i]==maxmask[i]):\n",
    "                zone[i]-=min(zone[i],contflow[i])\n",
    "            else:\n",
    "                zone[i]-=min(zone[i],changeflow[i])\n",
    "        elif (zone[i]):\n",
    "            times[i]+=TACT\n",
    "    return [maxmask, maxv]"
   ]
