    "def greed(mask,zone):\n",
    "    ans=0\n",
    "    for i in range(16):\n",
    "        if(prevmask[i]==mask[i]):\n",
    "            ans+=min(zone[i],contflow[i])*mask[i]*weights[i]*(((times[i]*timeweights[i])/30)**2)\n",
    "        else:\n",
    "            ans+=min(zone[i],changeflow[i])*mask[i]*weights[i]*(((times[i]*timeweights[i])/30)**2)\n",
    "    return ans"
   ]
  },
